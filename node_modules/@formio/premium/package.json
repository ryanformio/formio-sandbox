{
  "name": "@formio/premium",
  "version": "1.19.1",
  "description": "Premium components for form.io",
  "main": "lib/index.js",
  "module": "node",
  "publishConfig": {
    "registry": "https://pkg.form.io"
  },
  "scripts": {
    "test:coverage": "nyc --reporter=text mocha --reporter spec './{,!(node_modules)/**/}*.spec.js'",
    "test": "./node_modules/.bin/_mocha --require esm --require jsdom-global/register --require ejsify --reporter spec './{,!(node_modules)/**/}*.spec.ts'",
    "watch": "tsc -w",
    "build": "tsc && gulp templates && webpack && webpack --config webpack.prod.js && webpack --config webpack.server.js && node-sass ./src/sass/premium.form.scss ./dist/premium.css",
    "lint": "tslint -p .",
    "serve": "jekyll serve --config _config.yml,_config.dev.yml",
    "prepublish": "npm run build"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "files": [
    "dist",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formio/premium.git"
  },
  "keywords": [
    "Form.io",
    "components"
  ],
  "author": "Randall Knutson <randall@form.io>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/formio/premium/issues"
  },
  "homepage": "https://github.com/formio/premium#readme",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/ejs": "^3.1.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.7.10",
    "@types/sinon": "^10.0.10",
    "chai": "^4.3.6",
    "ejsify": "^1.0.0",
    "esm": "^3.2.25",
    "formiojs": "^4.15.0-rc.18",
    "gulp": "^4.0.2",
    "gulp-insert": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-template": "^5.0.0",
    "jsdom": "^17.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.4.0",
    "node-sass": "^6.0.1",
    "nyc": "^15.0.1",
    "pre-commit": "^1.2.2",
    "sinon": "^11.1.2",
    "ts-node": "^10.4.0",
    "ts-sinon": "^2.0.2",
    "tslint": "^6.1.2",
    "typescript": "4.1.6",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "peerDependencies": {
    "formiojs": "^4.13.3"
  },
  "dependencies": {
    "@juggle/resize-observer": "^3.4.0",
    "@zxing/library": "^0.18.6",
    "dayjs": "^1.10.7",
    "papaparse": "^5.3.1",
    "tooltip.js": "^1.3.3",
    "two.js": "0.7.1",
    "uuid": "^8.3.2",
    "vanilla-picker": "^2.12.1"
  }
}
